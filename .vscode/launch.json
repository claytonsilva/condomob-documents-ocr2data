{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "docling",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "args": [
        "analytical",
        "run",
        "${workspaceFolder}/files/page_30_preto_Demonstrativo_Analitico_de_Receitas_e_Despesas.pdf"
      ]
    },
    {
      "name": "split",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "args": [
        "spliter",
        "run",
        "${workspaceFolder}/files/2024-2.pdf",
        "--start=20",
        "--end=89"
      ]
    },
    {
      "name": "merger",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "args": [
        "merger",
        "run",
        "--path-dir",
        "${workspaceFolder}/output",
        "--output",
        "${workspaceFolder}/processed/merged.csv"
      ]
    },
    {
      "name": "analytical-reprocess",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "args": [
        "analytical",
        "reprocess",
        "${workspaceFolder}/output",
        "--output-dir",
        "${workspaceFolder}/output",
        "--file-type",
        ".csv"
      ]
    },
    {
      "name": "analytical-v2",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "args": [
        "analytical",
        "run",
        "${workspaceFolder}/files/2024-2.pdf",
        "--start=44",
        "--end=44",
        "--output-dir",
        "${workspaceFolder}/output",
        "--processed-dir",
        "${workspaceFolder}/processed",
        "--reprocess"
      ]
    }
  ]
}
